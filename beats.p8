------------------------- dat --
local t = 0;
local bdat = {}
bdat[4] = -- o
 { x=-19, y=3, sym="Ž", col=12 }
bdat[0] = -- left
 { x=-11, y=3, sym="‹", col= 8 }
bdat[2] = -- up
 { x= -3, y=3, sym="”", col=10 }
bdat[1] = -- right
 { x=  5, y=3, sym="‘", col= 9 }
bdat[5] = -- x
 { x= 13, y=3, sym="—", col=11 }

bdat[3] = -- down (unused)
 { x= -3, y=9, sym="ƒ", col=13 }

local odat = {
 bdat[4],
 bdat[0],
 bdat[2],
 bdat[1],
 bdat[5],
 bdat[3]
}
