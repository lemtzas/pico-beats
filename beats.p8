------------------------ help --
function btnt(i,a,b)
 return btn(i) and a or b
end

------------------------ init --
function _init()

end

---------------------- update --
function _update()
t=t+1
end

------------------------ draw --
function draw_map()
 local measurey = -90
 local measurex = 48
 local speed = 7 -- high = slow
 local offset = flr(t/speed)
 local interp = t/speed - offset
 -- beat map
 rectfill(46, measurey-1, 57, measurey+1, 5)
 for i=0,16 do
  map(i,0, measurex,16*8*i-96-offset, 1, 16)
 end

 -- button highlight
 for i=1,6 do
  b = odat[i]
  c = pget(measurex+i, measurey)
  if c ~= 0 and c ~= 5 then
   circfill(b.x+3, b.y+2, 2, b.col)
  end
 end

 -- lookahead
 printh(interp)
 for i=1,6 do
  b = odat[i]
  active = {}
  active[-1] = false
  for j=0,48 do
   ypos = 7 - (j * 4) + 4 * interp
   c = pget(measurex+i, measurey+j)
   pal(15, b.col)
   active[j] = c ~= 0 and c ~= 5
   new = active[j] and not active[j-1]
   sustained = active[j] and active[j-1]
   sustained2 = sustained and active[j-2]
   if sustained2 then
    spr(32, b.x,ypos+4)
    spr(32, b.x,ypos)
   elseif new then
    print(b.sym, b.x,ypos, b.col)
   end
  end
 end
 rectfill(-20,4,20,15,0)
end

function _draw()
-- setup
cls(); camera(-64,-96)

-- guide
line(0,0,0,-128,2)
line(-10,0,10,0,2)

draw_map()

-- keys
print("t: "..t,-63,-95,2)
for i=0,5 do
 b = bdat[i]
 print(b.sym, b.x, b.y,
  btnt(i,b.col,5))
end

-- who
print(who,-54, 20, 8)
spr(1,-63,20,1,2)

end

