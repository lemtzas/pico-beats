pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

------------------------- dat --
local t = 96
local lasttxt = ""
local score = 0
local active = {}
local measurey = -90
local measurex = 48
local odat = {
 { b=4, x=-19, y=3, -- o
   sym="Ž", col=12, d=false },
 { b=0, x=-11, y=3, -- left
   sym="‹", col= 8, d=false  },
 { b=2, x= -3, y=3, -- up
   sym="”", col=10, d=false  },
 { b=1, x=  5, y=3, -- right
   sym="‘", col= 9, d=false  },
 { b=5, x= 13, y=3, -- x
   sym="—", col=11, d=false  }}
 -- { b=3, x= -3, y=9, -- down
 --   sym="ƒ", col=13, d=false  }}

------------------------ help --

------------------------ init --
function _init()
 sfx(0)
end

---------------------- update --
function _update()
t=min(t+0.5, 4096)

cls(); beat_map()

-- lookahead
active = {}
for i=1,5 do
 active[i] = {}
 local last = { beat = false }
 for j=-6,48 do
  c = pget(measurex+i, measurey+j)
  ison = c > 5
  last = {
    beat = ison,
    new = ison and not last.beat,
    sustained = ison and last.beat
   }
   active[i][j] = last
   if j == 0 then
    odat[i].active = active[i][0]
   end
 end

 -- try to score
 b = odat[i]
 onnow = btn(b.b)
 if onnow then
  if b.active.sustained then
   score = score + 1
   lasttxt = "+1"
  end
  if not b.on then
   for j=0,2 do
    local early = active[i][j].new
    local late = active[i][-j].new
    if j < 1 then lasttxt = "perfect"; score = score + 99
    elseif early then lasttxt = "early"
    elseif late then lasttxt = "late"
    else lasttxt = "" end
    if early or late then
     score = score + 100
     break;
    end
   end
  end
 end
 b.on = onnow
end
end

------------------------ draw --
function beat_map()
 for i=0,16 do
  map(i,0, measurex,128*i-flr(t), 1,16)
 end
end

function draw_map()
 -- lookahead
 for i=1,5 do
  b = odat[i]
  pal(15, b.col)
  for j=0,48 do
   dat = active[i][j]
   ypos = 4 * (t - flr(t) - j)
   if dat.sustained then
    spr(32, b.x,ypos)
   elseif dat.new then
    print(b.sym, b.x,ypos, b.col)
   end
  end
 end
end

function _draw()
cls(); camera(-64,-96)
map(16,0, -64,-96, 16,16)

beat_map()
draw_map()

print("t: "..t,-63,-95,2)
print(lasttxt.."\n"..score,-9,20,2)
for i=1,5 do
 b = odat[i]
 circfill(b.x+3, b.y+2, 2,
  b.active.beat and b.col or 0)
 print(b.sym, b.x, b.y,
  btn(b.b) and b.col or 5)
end
--
-- -- who
print("lemtzas\nnolan_exe",
 -54, 20, 8)

end

